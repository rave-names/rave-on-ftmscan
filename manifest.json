{
  "name": "Rave Names on FTMScan",
  "description": "View Rave Names on FTMScan!",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "content_scripts": [{
    "js": ["b-main.js"],
    "matches": ["https://ftmscan.com/*"]
  }],
  "content_security_policy": {
    "extension_pages": "default-src 'self'"
  },
  "icons": {
    "16": "img.png",
    "32": "img.png",
    "48": "img.png",
    "128": "img.png"
  }
}
